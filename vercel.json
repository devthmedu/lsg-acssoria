{
  "version": 2,
  "builds": [
    {
      "src": "public/video/**",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.{js,jsx,ts,tsx}",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/video/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/public/video/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
